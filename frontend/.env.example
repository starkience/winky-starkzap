# Privy App ID (from console.privy.io)
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:3000

# Network: mainnet | sepolia
NEXT_PUBLIC_NETWORK=mainnet

# Winky contract address (optional, defaults based on network)
# NEXT_PUBLIC_WINKY_CONTRACT_ADDRESS=0x004918f613695bbd6ad40b853564b1fc6ab7e1630ecbc2c7db7705cdb937983f

# Privy login methods (comma-separated): email, google, twitter, discord
# NEXT_PUBLIC_PRIVY_LOGIN_METHODS=email,google,twitter

# Pusher (for live feed)
# PUSHER_APP_ID=...
# PUSHER_KEY=...
# PUSHER_SECRET=...
# NEXT_PUBLIC_PUSHER_KEY=...
# NEXT_PUBLIC_PUSHER_CLUSTER=...
